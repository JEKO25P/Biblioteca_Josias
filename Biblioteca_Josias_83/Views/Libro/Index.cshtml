@model IEnumerable<Biblioteca_Josias_83.Models.Libro>

<h2>Lista de Libros</h2>

<table>
    <tr>
        <th>ID</th>
        <th>Título</th>
        <th>Autor</th>
        <th>Año de Publicación</th>
    </tr>
    @foreach (var libro in Model)
    {
        <tr>
            <td>@libro.Id</td>
            <td>@libro.Titulo</td>
            <td>@libro.Autor</td>
            <td>@libro.AnioPublicacion</td>
        </tr>
    }
</table>
